{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "version": "1.0.0",
  "author": "{{ author }}",
  "license": "{{ license }}",
  "private": false,
  "main": "lib/{{ name }}.min.js",
  "scripts": {
    {{#if storybook}}
    "dev": "npm run storybook",
    "build:demo": "npm run build-storybook",
    "build-storybook": "build-storybook -c .storybook -o docs",
    "storybook": "storybook",
    {{else}}
    "dev": "webpack-dev-server --hot --inline --config build/webpack.config.dev.js --mode development",
    "build:demo": "webpack --config build/webpack.config.prod.js --mode production",
    {{/if}}
    "build": "webpack --config build/webpack.config.js --mode production"
  },
  "peerDependencies": {
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "serve": "^6.5.6",
    {{#if storybook}}
    "storybook": "^1.0.0",
    {{/if}}
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-loader": "14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "vue": "^2.5.16"
  }
}
